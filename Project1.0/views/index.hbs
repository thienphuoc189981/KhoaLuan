<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
<div id="show"></div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script>
    $.ajax({
        url: "http://localhost:1337/api/job/1",
        type: "GET",
        crossDomain: true,
        dataType: 'json'
    }).done(function (obj) {
        console.log(obj);
        $('#show').html(obj.title);
    });;
</script>