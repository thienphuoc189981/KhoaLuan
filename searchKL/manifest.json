{
    "manifest_version": 2,
    "name": "Search Extension",
    "description": "Search from other website",
    "author": "ThuyLe",
    "version": "1.0.0",
    "icons": {
        "16": "images/app.png",
        "48": "images/app.png",
        "128": "images/app.png"
    },
    "browser_action": {
        "default_icon": "images/app.png",
        "default_title": "Search Extension"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": [
        "http://*/",
        "https://*/"
    ],
    "content_scripts": [
    {
      "matches": ["http://localhost:1337/searchExtension"],
      "js": ["js/angular.js", "js/angular-filter.js", "js/ui-bootstrap-tpls.js", "js/jquery.min.js", "js/pagination.js"]
    }
    ],
    "web_accessible_resources": [
      "images/app.png"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
